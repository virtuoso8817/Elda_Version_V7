        <activity
            android:name=".CallHandlerActivity"
            android:exported="false" />
        <activity
            android:name=".PersonAdapter"
            android:exported="false" />
        <activity
            android:name=".CallSetActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />
